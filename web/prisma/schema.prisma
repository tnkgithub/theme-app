generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model poster {
  id                        String    @id
  title                     String
  explanation               String?
  somCoordinate             Int
  representationsCoordinate Int?
  objWord                   obj_word[]
}

model obj_word {
  word     String
  posterId String
  poster   poster @relation(fields: [posterId], references: [id])

  @@id([word, posterId])
}
