generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Poster {
  id                        String    @id
  title                     String
  discription               String?
  somCoordinate             Int @map("som_coordinate")
  representationsCoordinate Int? @map("representations_coordinate")
  objectWord                ObjectWord[]
  @@map("posters")
}

model ObjectWord {
  word     String
  posterId String @map("poster_id")
  poster   Poster @relation(fields: [posterId], references: [id])

  @@id([word, posterId])
  @@map("object_words")
}

